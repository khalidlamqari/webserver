http {

    error_page 599 700.html;
    error_page  500 999.html;
    error_page  404 4x04.html;

    server {

        server_names "  \\\"example\'.com " exa\;mple.co;
        allowed_methods POST GET ;

        listen 8888 8999 ;

        root ./z_site/ ;

        index index.html ;

        
        location /abt/ {
            root ./z_site/about ;
            index about.html ;
        }

        location = /abt/ {
            root ./z_site/ ;
            index indehhhx.html ;
        }

        location / {
            index index.html ;
        }

        location /about {
            return 301 https://www.w3schools.com/php/phptryit.asp?filename=tryphp_intro ;
        }

        location /signin/ {
            return 302 /abt/ ;
        }

        location /bad {
            return 400 "bad request" ;
        }

    }

}
